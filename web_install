if [ -z $dev ]; then
echo "Downloading from the master branch"
wget https://raw.githubusercontent.com/alx99/arch-ai/master/sharedfuncs;wget https://raw.githubusercontent.com/alx99/arch-ai/master/common_pkgs;wget https://raw.githubusercontent.com/alx99/arch-ai/master/setup;wget https://raw.githubusercontent.com/alx99/arch-ai/master/install;chmod +x install
else
echo "Downloading from the develop branch"
wget https://raw.githubusercontent.com/ALX99/arch-ai/develop/sharedfuncs;wget https://raw.githubusercontent.com/alx99/arch-ai/develop/common_pkgs;wget https://raw.githubusercontent.com/alx99/arch-ai/develop/setup;wget https://raw.githubusercontent.com/alx99/arch-ai/develop/install;chmod +x install
fi
